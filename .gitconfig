[remote "template"]
	url = https://github.com/t3-oss/create-t3-turbo.git
	fetch = +refs/heads/*:refs/remotes/template/*

[gitflow "branch"]
	master = main
	develop = develop

[gitflow "prefix"]
	feature = feature/
	release = release/
	hotfix = hotfix/
	support = support/
	versiontag =
	bugfix = bugfix/

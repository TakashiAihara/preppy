{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": [
    "**/.env"
  ],
  "globalEnv": [
    "CI",
    "DATABASE_URL",
    "NODE_ENV",
    "SKIP_ENV_VALIDATION",
    "VERCEL",
    "VERCEL_URL"
  ],
  "pipeline": {
    "//#clean": {
      "cache": false
    },
    "build": {
      "dependsOn": [
        "^build",
        "^db:generate"
      ],
      "outputs": [
        ".next/**"
      ]
    },
    "clean": {
      "cache": false
    },
    "db:generate": {
      "cache": false,
      "inputs": [
        "prisma/schema.prisma"
      ]
    },
    "db:push": {
      "cache": false,
      "inputs": [
        "prisma/schema.prisma"
      ]
    },
    "db:seed": {
      "cache": false,
      "inputs": [
        "prisma/schema.prisma"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {},
    "lint:fix": {},
    "type-check": {
      "cache": false,
      "dependsOn": [
        "^db:generate"
      ]
    }
  }
}
